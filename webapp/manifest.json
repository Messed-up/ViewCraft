{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.vodafone.MyProject",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"Northwind.svc": {
				"uri": "/Northwind_test/V2/Northwind/Northwind.svc/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.vodafone.MyProject.view.Base",
			"type": "XML",
			"async": true,
			"id": "Base"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.vodafone.MyProject.i18n.i18n"
				}
			},
			"productsModel": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "Northwind.svc",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.vodafone.MyProject.view",
				"controlAggregation": "content",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "TargetBase",
					"pattern": "RouteBase",
					"target": [
						"TargetBase"
					]
				},
				{
					"name": "Home",
					"pattern": "Home",
					"target": [
						"Home"
					]
				},
				{
					"name": "Detail",
					"pattern": "Detail",
					"target": [
						"Detail"
					]
				},
				{
					"name": "Dynamic",
					"pattern": "Dynamic",
					"target": [
						"Dynamic"
					]
				}
			],
			"targets": {
				"TargetBase": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "Base",
					"viewName": "Base"
				},
				"Home": {
					"viewType": "XML",
					"viewName": "Home",
					"clearControlAggregation": true
				},
				"Detail": {
					"viewType": "XML",
					"viewName": "Detail",
					"clearControlAggregation": true
				},
				"Dynamic": {
					"viewType": "XML",
					"viewName": "Dynamic",
					"clearControlAggregation": true
				}
			}
		}
	}
}